<%= render :partial => "dashboard/navigation_sidebar" %>
<div class="span9">
  <div class="well well-small">
    <h4 style="margin-left: 15px;"><i class="icon-thumbs-up"/></i> Liked products</h4>
  </div>

  <% unless flash[:notice].blank? %>
    <div class="alert alert-success">
      <button type="button" class="close" data-dismiss="alert">&times;</button>
      <%= flash[:notice].html_safe %>
    </div>
  <% end %>

  <% if @likes.blank? %>
    <div class="alert alert-info">
      <button type="button" class="close" data-dismiss="alert">&times;</button>
        You haven't liked any product yet.
    </div>
  <% else %>
    <%= render :partial => 'shared/users_liked_products', :locals => {:likes => @likes} %>
  <% end %>

</div>